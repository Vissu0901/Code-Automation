<xsd:schema xmlns="http://www.irs.gov/efile" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.irs.gov/efile" elementFormDefault="qualified" attributeFormDefault="unqualified" version="1.0">
	<xsd:annotation>
		<xsd:documentation>
			<Description>State e-file Business Income Tax Schema</Description>
		</xsd:documentation>
	</xsd:annotation>
	<xsd:include schemaLocation="../StateBusiness/BusinessReturnHeaderState.xsd"/>
	<xsd:include schemaLocation="../StateBusiness/AuthenticationHeader.xsd"/>
	<xsd:include schemaLocation="ReturnDataRI1120C.xsd"/>
	<xsd:include schemaLocation="../Common/BinaryAttachment.xsd"/>
	<xsd:include schemaLocation="../Common/FinancialTransaction.xsd"/>
	<!-- Business Return - 1120 -->
	<xsd:element name="ReturnState">
		<xsd:annotation>
			<xsd:documentation>1120 Individual Return - wraps around Return Header and Return Data</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element ref="AuthenticationHeader" minOccurs="0"/>
				<xsd:element ref="ReturnHeaderState"/>
				<!--ReturnDataState to be built by each state -->
				<xsd:element ref="ReturnDataState"/>
				<xsd:element ref="BinaryAttachment" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element ref="FinancialTransaction" minOccurs="0"/> 
			</xsd:sequence>
			<xsd:attribute name="stateSchemaVersion" type="String50Type" use="required" fixed="RIBusiness2022V.01"/>
			<!-- Version of the state schema set used for the filing-->
		</xsd:complexType>
	</xsd:element>
</xsd:schema>
