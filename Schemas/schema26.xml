		<xsd:sequence>
			<!-- Number of shares  -->
			<xsd:element name="NumShares" type="IntegerNNType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Number of shares</Description>
						<LineNumber>Line 1</LineNumber>
						<FormNumber>Form1120C-1120S</FormNumber>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!--Par Value  -->
			<xsd:element name="ParValue" type="USDecimalAmountType">
				<xsd:annotation>
