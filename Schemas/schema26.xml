						<Description>Par Value</Description>
						<LineNumber>Line 2</LineNumber>
						<FormNumber>Form1120C-1120S</FormNumber>>
					</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<!--Franchise Tax  -->
			<xsd:element name="FranTax" type="USDecimalAmountType">
				<xsd:annotation>
					<xsd:documentation>
						<Description>Franchise Tax not less then $500</Description>
						<LineNumber>Line 7</LineNumber>
						<FormNumber>Form1120C-1120S</FormNumber>>
					</xsd:documentation>
				</xsd:annotation>
