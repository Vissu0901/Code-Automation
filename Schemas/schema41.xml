	<xsd:simpleType name="RISmallRatioType">
		<xsd:annotation>
			<xsd:documentation>A fraction between 0 and 1 that allows up to 2 decimal places</xsd:documentation>
		</xsd:annotation>
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="3"/>
			<xsd:fractionDigits value="2"/>
			<xsd:minInclusive value="0.00"/>
			<xsd:maxInclusive value="1.00"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RIRatioType">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="5"/>
			<xsd:fractionDigits value="4"/>
			<xsd:minInclusive value="0.0000"/>
			<xsd:maxInclusive value="1.0000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RICorpRatioType">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="7"/>
			<xsd:fractionDigits value="6"/>
			<xsd:minInclusive value="0.000000"/>
			<xsd:maxInclusive value="1.000000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RICorpTotalRatioType">
		<xsd:restriction base="xsd:decimal">
			<xsd:totalDigits value="7"/>
			<xsd:fractionDigits value="6"/>
			<xsd:minInclusive value="0.000000"/>
			<xsd:maxInclusive value="3.000000"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="RIString20Type">
		<xsd:restriction base="xsd:string">
